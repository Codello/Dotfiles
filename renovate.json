{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "regexManagers": [{
    "fileMatch": ["(^|\\/).chezmoiexternal.toml$"],
    "matchStrings": ["url\\s*=\\s*\"https:\\/\\/github.com\\/(?<depName>[^\\/]+\\/[^\\/]+)\\/archive\\/refs\\/tags\\/(?<currentValue>\\d+(\\.\\d+)*).zip\""],
    "datasourceTemplate": "github-tags"
  }]
}
